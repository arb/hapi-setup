{
  "name": "hapi-setup",
  "version": "2.0.0",
  "description": "hapi plugin for viewing the server configuration",
  "author": "Continuation Labs <contact@continuation.io> (http://continuation.io/)",
  "main": "lib/index.js",
  "homepage": "https://github.com/continuationlabs/hapi-setup",
  "repository": {
    "type": "git",
    "url": "https://github.com/continuationlabs/hapi-setup.git"
  },
  "bugs": {
    "url": "https://github.com/continuationlabs/hapi-setup/issues"
  },
  "license": "MIT",
  "dependencies": {
    "autoprefixer-stylus": "0.8.0",
    "hoek": "2.x.x",
    "jade": "1.11.x",
    "jeet": "6.1.x",
    "rupture": "0.6.1",
    "stylus": "0.52.4",
    "vision": "3.x.x"
  },
  "devDependencies": {
    "code": "1.x.x",
    "hapi": "9.x.x",
    "inert": "3.x.x",
    "joi": "6.x.x",
    "lab": "5.x.x",
    "semistandard": "7.x.x"
  },
  "scripts": {
    "build": "stylus style/main.styl -o public/styles.css -I ./node_modules/jeet/stylus -u ./node_modules/autoprefixer-stylus -u ./node_modules/rupture -c",
    "dev": "node dev-server & stylus assets/style/main.styl -o build/styles.css -I ./node_modules/jeet/stylus -u ./node_modules/autoprefixer-stylus -u ./node_modules/rupture -c -w",
    "prepublish": "npm run build",
    "test": "semistandard && lab -v -t 100 -a code"
  },
  "keywords": [
    "hapi",
    "plugin",
    "documentation",
    "setup",
    "config"
  ]
}
